{
  "name": "@feature-hub/module-loader-amd",
  "version": "3.5.2",
  "description": "A FeatureAppManager-compatible AMD module loader.",
  "homepage": "https://feature-hub.io/",
  "bugs": {
    "url": "https://github.com/feature-hub/feature-hub/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feature-hub/feature-hub.git"
  },
  "license": "MIT",
  "author": "Accenture Song Build Germany GmbH",
  "sideEffects": false,
  "exports": {
    ".": {
      "@feature-hub:bundler": "./src/index.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/cjs/index.d.ts",
  "files": [
    "lib",
    "!__tests__"
  ],
  "dependencies": {
    "systemjs": "^6.14.2"
  },
  "devDependencies": {
    "@types/systemjs": "^6.13.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "module-loader-amd"
  }
}
